{
  "name": "@zero-company/zero-lib-react",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "concurrently \"pnpm storybook:dev\" \"pnpm next:dev\" \"pnpm lib:dev\"",
    "build": "concurrently \"pnpm storybook:build\" \"pnpm next:build\" \"pnpm lib:build\"",
    "lib:dev": "tsup --watch",
    "lib:build": "tsup",
    "next:dev": "next dev -p 8001",
    "next:build": "next build",
    "next:start": "next start",
    "storybook:dev": "storybook dev -p 8002 --no-open",
    "storybook:build": "storybook build --output-dir dist-storybook",
    "tailwind:dev": "tailwindcss -i src/globals.css -o dist/lib/zero-lib-react.css --watch",
    "tailwind:build": "tailwindcss -i src/globals.css -o dist/lib/zero-lib-react.css --minify"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.424.0",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-interactions": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/addon-onboarding": "^8.2.2",
    "@storybook/addon-viewport": "^8.2.8",
    "@storybook/blocks": "^8.2.2",
    "@storybook/manager-api": "^8.2.2",
    "@storybook/nextjs": "^8.2.2",
    "@storybook/react": "^8.2.2",
    "@storybook/test": "^8.2.2",
    "@storybook/theming": "^8.2.2",
    "@tailwindcss/typography": "^0.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8",
    "storybook": "^8.2.2",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "typescript": "^5"
  }
}
