  "scripts": {
    "dev": "concurrently \"pnpm next:dev\" \"pnpm lib:dev\"",
    "build": "concurrently \"pnpm storybook:build\" \"pnpm next:build\" \"pnpm lib:build\"",
    "lib:dev": "tsup --watch",
    "lib:build": "tsup && pnpm tailwind:build",
    "next:dev": "next dev -p 8001",
    "next:build": "cross-env NEXT_OUTPUT=export next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "storybook:dev": "storybook dev -p 8002 --no-open",
    "storybook:build": "storybook build --output-dir dist-storybook",
    "tailwind:dev": "tailwindcss -i src/globals.css -o dist/lib/zero-lib-react.css --watch",
    "tailwind:build": "tailwindcss -i src/globals.css -o dist/lib/zero-lib-react.css --minify",
    "shadcn": "shadcn",
    "turbo": "turbo",
    "codegen": "graphql-codegen --config codegen.ts"
